{"ast":null,"code":"export default {\n  name: \"Idea\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 10,\n      userid: \"\",\n      ideaTitle: \"\",\n      form: {},\n      dialogFormVisible: false,\n      deleteDialogFormVisible: false,\n      headerBg: 'headerBg'\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.request.get(\"http://localhost:9090/idea/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          userid: this.userid,\n          title: this.ideaTitle\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          console.log(res);\n          this.tableData = res.data.data;\n          console.log(this.tableData);\n          this.total = res.data.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    reset() {\n      this.userid = \"\";\n      this.ideaTitle = \"\";\n      this.pageNum = 1;\n      this.pageSize = 10;\n      this.load();\n    },\n    deleteIdea() {\n      console.log(this.form);\n      this.request.get(\"http://localhost:9090/idea/delete\", {\n        params: {\n          id: this.form.id\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"删除成功\");\n          console.log(res);\n          this.deleteDialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleContent(row) {\n      this.form = row;\n      this.dialogFormVisible = true;\n    },\n    handleDelete(row) {\n      this.form = row;\n      this.deleteDialogFormVisible = true;\n    },\n    handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"mappings":"AA4DA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACA;QACAC;UACAX;UACAC;UACAC;UACAU;QACA;MACA;QAEA;UACAC;UACA;UACAA;UACA;QACA,OACA;UACA;QACA;MACA;IACA;IACAC;MACA;MACA;MACA;MACA;MACA;IACA;IACAC;MACAF;MACA;QACAF;UACAK;QACA;MACA;QACA;UACA;UACAH;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IACAI;MACA;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACAN;MACA;MACA;IACA;IACAO;MACAP;MACA;MACA;IACA;EACA;AACA","names":["name","data","tableData","total","pageNum","pageSize","userid","ideaTitle","form","dialogFormVisible","deleteDialogFormVisible","headerBg","created","methods","load","params","title","console","reset","deleteIdea","id","handleContent","handleDelete","handleSizeChange","handleCurrentChange"],"sourceRoot":"src/views/front","sources":["Idea.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入用户名\" suffix-icon=\"el-icon-message\" v-model=\"userid\" class=\"ml-5\"></el-input>\r\n      <el-input style=\"width: 200px; margin-left: 10px\" placeholder=\"请输入标题\" suffix-icon=\"el-icon-position\" v-model=\"ideaTitle\" class=\"ml-5\"></el-input>\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\" style=\"margin-left: 10px\" size=\"small\">搜索</el-button>\r\n      <el-button class=\"ml-5\" type=\"info\" @click=\"reset\" style=\"margin-left: 10px\" size=\"small\">重置</el-button>\r\n    </div>\r\n<!--    -->\r\n<!--    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\">-->\r\n<!--      <el-table-column prop=\"id\" label=\"ID\" width=\"80\"></el-table-column>-->\r\n<!--      <el-table-column prop=\"uid\" label=\"用户名\" width=\"140\"></el-table-column>-->\r\n<!--      <el-table-column prop=\"title\" label=\"标题\"></el-table-column>-->\r\n<!--      <el-table-column prop=\"type\" label=\"类型\"></el-table-column>-->\r\n<!--      <el-table-column prop=\"pubTime\" label=\"发布时间\"></el-table-column>-->\r\n<!--      <el-table-column prop=\"finTime\" label=\"最后修改时间\"></el-table-column>-->\r\n<!--      <el-table-column label=\"操作\"  width=\"200\" align=\"center\">-->\r\n<!--        <template slot-scope=\"scope\">-->\r\n<!--          <el-button type=\"success\" @click=\"handleContent(scope.row)\">查看内容 <i class=\"el-icon-edit\"></i></el-button>-->\r\n<!--        </template>-->\r\n<!--      </el-table-column>-->\r\n<!--    </el-table>-->\r\n\r\n    <div style=\"margin: 10px 0\">\r\n\r\n      <div style=\"padding: 20px 0; border-bottom: 1px dashed #666\" v-for=\"item in tableData\" :key=\"item.id\">\r\n        <div>\r\n          {{ item.title }}\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n\r\n\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, prev, pager, next\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n    <el-dialog title=\"创意信息\" :visible.sync=\"dialogFormVisible\" width=\"50%\">\r\n      <el-form label-width=\"80px\">\r\n        <el-form-item label=\"内容\">\r\n          <el-input type=\"textarea\" :autosize=\"{ minRows:8, maxRows:16}\" v-model=\"form.content\" autocomplete=\"off\" ></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">关 闭</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Idea\",\r\n  data(){\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      userid:\"\",\r\n      ideaTitle:\"\",\r\n      form:{},\r\n      dialogFormVisible:false,\r\n      deleteDialogFormVisible:false,\r\n      headerBg: 'headerBg'\r\n    }\r\n  },\r\n  created() {\r\n    this.load();\r\n  },\r\n  methods : {\r\n    load(){\r\n      this.request.get(\"http://localhost:9090/idea/page\",{\r\n        params:{\r\n          pageNum:this.pageNum,\r\n          pageSize:this.pageSize,\r\n          userid:this.userid,\r\n          title:this.ideaTitle\r\n        }\r\n      }).then(res=>{\r\n\r\n        if(res.code==='200'){\r\n          console.log(res)\r\n          this.tableData=res.data.data;\r\n          console.log(this.tableData)\r\n          this.total=res.data.total;\r\n        }\r\n        else{\r\n          this.$message.error(res.msg);\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.userid = \"\"\r\n      this.ideaTitle = \"\"\r\n      this.pageNum = 1\r\n      this.pageSize = 10\r\n      this.load()\r\n    },\r\n    deleteIdea(){\r\n      console.log(this.form);\r\n      this.request.get(\"http://localhost:9090/idea/delete\",{\r\n        params:{\r\n          id:this.form.id\r\n        }\r\n      }).then(res=>{\r\n        if(res.code==='200'){\r\n          this.$message.success(\"删除成功\")\r\n          console.log(res)\r\n          this.deleteDialogFormVisible=false\r\n          this.load();\r\n        }else{\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleContent(row){\r\n      this.form=row\r\n      this.dialogFormVisible=true;\r\n    },\r\n    handleDelete(row){\r\n      this.form=row\r\n      this.deleteDialogFormVisible=true;\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.headerBg {\r\n  background: #eee!important;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}