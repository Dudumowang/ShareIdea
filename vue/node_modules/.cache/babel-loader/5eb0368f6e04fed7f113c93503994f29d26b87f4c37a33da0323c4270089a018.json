{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"CreateIdea\",\n  data() {\n    return {\n      idea: {},\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\n    };\n  },\n  methods: {\n    // 绑定@imgAdd event\n    imgAdd(pos, $file) {\n      // 将图片上传到服务器\n      const formData = new FormData();\n      formData.append('file', $file);\n      axios({\n        url: 'http://localhost:9090/file/upload',\n        method: 'post',\n        data: formData,\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      }).then(res => {\n        // 将返回的url替换到文本原位置\n        if (res.status === 200) {\n          this.$refs.md.$img2Url(pos, res.data);\n        }\n      });\n    },\n    createIdea() {\n      this.request.post('http://localhost:9090/idea/save', this.idea, this.user).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"创意分享成功！\");\n          Object.assign(this.idea, {});\n        } else {\n          this.$message.error(res.msg());\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAyBA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACAC;MACA;MACA;MACAC;MACAC;QACAC;QACAC;QACAR;QACAS;UAAA;QAAA;MACA;QACA;QACA;UACA;QACA;MACA;IACA;IAEAC;MACA,2EACAC;QACA;UACA;UACAC;QACA;UACA;QACA;MACA;IACA;EACA;AACA","names":["name","data","idea","user","methods","imgAdd","formData","axios","url","method","headers","createIdea","then","Object"],"sourceRoot":"src/views/front","sources":["CreateIdea.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-form label-width=\"80px\" size=\"small\" style=\"margin-top: 20px\">\r\n\r\n      <el-form-item label=\"创意标题\">\r\n        <el-input v-model=\"idea.title\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"创意内容\">\r\n        <mavon-editor ref=\"md\" v-model=\"idea.content\" :ishljs=\"true\" @imgAdd=\"imgAdd\" style=\"height: 550px\"/>\r\n      </el-form-item>\r\n\r\n    </el-form>\r\n\r\n    <div style=\"text-align: right\">\r\n      <el-button type=\"primary\" @click=\"createIdea\">确认发布</el-button>\r\n    </div>\r\n\r\n\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"CreateIdea\",\r\n  data() {\r\n    return {\r\n      idea : {},\r\n      user : localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\r\n    }\r\n  },\r\n  methods: {\r\n    // 绑定@imgAdd event\r\n    imgAdd(pos, $file) {\r\n      // 将图片上传到服务器\r\n      const formData = new FormData();\r\n      formData.append('file', $file);\r\n      axios({\r\n        url:'http://localhost:9090/file/upload',\r\n        method:'post',\r\n        data:formData,\r\n        headers: {'Content-Type': 'multipart/form-data'},\r\n      }).then(res => {\r\n        // 将返回的url替换到文本原位置\r\n        if (res.status === 200){\r\n          this.$refs.md.$img2Url(pos, res.data)\r\n        }\r\n      })\r\n    },\r\n\r\n    createIdea(){\r\n      this.request.post('http://localhost:9090/idea/save',this.idea, this.user)\r\n          .then( res => {\r\n            if (res.code === '200') {\r\n              this.$message.success(\"创意分享成功！\")\r\n              Object.assign(this.idea, {})\r\n            } else {\r\n              this.$message.error(res.msg())\r\n            }\r\n          })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}